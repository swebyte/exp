{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Nuxt.js in debug mode",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev"
            ],
            "env": {
                "DEBUG": "nuxt:*"
            },
            "console": "integratedTerminal",
            "port": 9229
        }
    ]
}